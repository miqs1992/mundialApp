<div class='row'>
  <div class='col-lg-12'>
    <div class="box box-primary text-center">
      <div class="box-header">
        <h3 class="box-title">Moje typy</h3>
      </div>
      <div class="box-body table-responsive no-padding">
        <table class="table table-striped table-bordered">
          <tbody>
            <tr>
              <th>Mecz</th>
              <th>PoczÄ…tek meczu</th>
              <th>Typ</th>
              <th>Wynik</th>
              <th>Punkty</th>
            </tr>
            <% @bets.each do |bet| %>
              <tr>
                <td><%= bet.match.print_teams.html_safe %></td>
                <td><%= l(bet.match.start_time, format: :short) %></td>
                <td><%= bet.print %></td>
                <td><%= bet.match.print_score %></td>
                <td><%= bet.points %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div> 